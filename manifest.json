{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon19.png",
      "default_title": "切换https"
   },
   "description": "切换使用https打开网页，并且记住选择，还能帮助你找出请求失败的域名",
   "icons": {
      "128": "icon.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "17",
   "name": "HTTPS Switcher",
   "options_page": "options.html",
   "permissions": [ "*://*/", "tabs", "webRequest", "webRequestBlocking" ],
   "version": "1.0.0"
}
